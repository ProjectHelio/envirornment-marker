{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "An extension to mark which environment you're working on.",
  "version": "0.0.1",
  "browser_action": {
    "default_popup": "html/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content.js"
      ]
    }
  ],
  "options_page": "html/options.html",
  "default_locale": "en"
}